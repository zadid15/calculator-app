<selector xmlns:android="http://schemas.android.com/apk/res/android">
<!-- Warna saat tombol ditekan -->
<item android:state_pressed="true">
    <shape android:shape="oval">
        <solid android:color="#999999"/> <!-- Warna lebih gelap saat ditekan -->
        <size android:width="85dp" android:height="85dp"/>
    </shape>
</item>

<!-- Warna default -->
<item>
    <shape android:shape="oval">
        <solid android:color="#A6A6A6"/> <!-- Warna oranye default -->
        <size android:width="85dp" android:height="85dp"/>
    </shape>
</item>
</selector>



